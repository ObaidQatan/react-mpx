{
  "name": "react-mpx",
  "version": "0.1.1",
  "description": "Run and export multiple React projects from a single codebase",
  "keywords": [
    "vite",
    "react",
    "multiproject",
    "cli",
    "vite-plugin",
    "monorepo"
  ],
  "homepage": "https://github.com/ObaidQatan/react-mpx",
  "bugs": {
    "url": "https://github.com/ObaidQatan/react-mpx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ObaidQatan/react-mpx.git"
  },
  "license": "MIT",
  "author": "Obaid Qatan ogaten27@gmail.com",
  "type": "module",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "bin": {
    "react-mpx": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "@eslint/js": "^9.21.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-formatter-pretty": "^6.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "typescript-eslint": "^8.26.0",
    "husky": "^9.1.7"
  }
}
